{
	"name": "@wezom/stylelint-config-scss",
	"version": "0.1.0-beta.0",
	"description": "Stylelint shareable config",
	"main": ".stylelintrc.json",
	"files": [
		".stylelintrc"
	],
	"scripts": {
		"auto-doc-rules": "node ../../utils/autoDocRules.js",
		"test": "jest",
		"test:autofix": "stylelint \"./tests/fixtures/*.valid.scss\" --fix",
		"check-valid": "stylelint \"./tests/fixtures/*.valid.scss\"",
		"check-invalid": "stylelint \"./tests/fixtures/comment-whitespace-inside.invalid.css\""
	},
	"license": "BSD-3-Clause",
	"author": {
		"name": "Oleg Dutchenko",
		"email": "dutchenko.o.dev@gmail.com"
	},
	"keywords": [
		"@wezom",
		"code-style",
		"stylelint",
		"stylelintconfig"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"stylelint-scss": "^3.18.0"
	}
}
