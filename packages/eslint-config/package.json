{
	"name": "@wezom/eslint-config",
	"version": "0.3.0-beta.0",
	"description": "ESLint shareable config",
	"main": ".eslintrc.json",
	"files": [
		".eslintrc.json"
	],
	"scripts": {
		"auto-doc-rules": "node ../../utils/autoDocRules.js && prettier ./RULES.md --write",
		"test": "jest",
		"test:autofix": "eslint \"./tests/fixtures/*.valid.js\" --fix",
		"check-valid": "eslint \"./tests/fixtures/*.valid.js\"",
		"check-invalid": "eslint \"./tests/fixtures/*.invalid.js\""
	},
	"license": "BSD-3-Clause",
	"author": {
		"name": "Oleg Dutchenko",
		"email": "dutchenko.o.dev@gmail.com"
	},
	"keywords": [
		"@wezom",
		"code-style",
		"eslint",
		"eslintconfig"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1"
	}
}
